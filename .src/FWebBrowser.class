Public Sub Form_Open()
  
  FWebBrowser.Center
  FWebBrowser.Width = 800
  FWebBrowser.Height = 600
'  WebView1.UserAgent = "Mozilla/5.0 (Linux; U; Android 4.0.3; ko-kr; LG-L160L Build/IML74K) AppleWebkit/534.30 (KHTML, like Gecko) Version/4.0 Mobile Safari/534.30"
  WebSettings[WebSettings.PluginsEnabled] = True
  
If FMain.URLKryFromItem_Boolean = True Then
  WebView1.Url = FMain.URLKryFromItem
  FMain.URLKryFromItem_Boolean = False
  Else
  WebView1.Url = "Html1.html"
Endif

End

Public Sub ToolButton_Previous_Click()
  
  WebView1.Back()
  
End

Public Sub ToolButton_Next_Click()
  
  WebView1.Forward()
  
End

Public Sub ToolButton_Navigate_Click()
  
  WebView1.Url = TextBox_URL.Text
  
End

Public Sub ToolButton_Refresh_Click()
  
  WebView1.Reload()
  
End

Public Sub ToolButton_Home_Click()
  
  WebView1.Url = "http://m.youtube.com"
  
End

Public Sub ToolButton_CopyURL_Click()
  
    FMain.TextBox_VideoURL.text = TextBox_URL.Text
    FMain.Add_VideoURL()

End


Public Sub WebView1_Status()

    WebStatus.Text = WebView1.Status

End
Public Sub TextBox_URL_KeyPress()
  
  If Key.code = Key.Return Or Key.code = Key.Enter Then
    WebView1.Url = TextBox_URL.Text
  Endif
  
End

Public Sub WebView1_Link(Url As String)

   WebStatus.Text = Url

End

Public Sub WebView1_Progress()

   TextBox_URL.Text = WebView1.Url

End
